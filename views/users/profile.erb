<div id="switch_bg">
  <div id="left_con"><div id="left_button"></div></div>
  <div id="right_con"></div>
  <div class="user_articles">
    <% articles = Article.where(user_id: params[:id]) %>
    <% articles.each do |article|%>
    <div class="article">
      <h3><%= article.title %></h3>
      <p><%= article.content %></p>
    </div>
    <% end %>
  </div>
</div>
